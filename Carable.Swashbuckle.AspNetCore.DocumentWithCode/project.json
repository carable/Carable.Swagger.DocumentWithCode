{
  "version": "0.0.5-*",
  "description": "Allows you to customize your swagger documentation using code, including support for providing multiple examples for responses and parameters.",

  "packOptions": {
    "releaseNotes": "Removed dependency on netcoreapp1.1",
    "requireLicenseAcceptance": false,
    "licenseUrl": "https://github.com/Carable/Carable.Swagger.DocumentWithCode/LICENSE",
    "owners": [
      "Carable"
    ],
    "iconUrl": "",
    "projectUrl": "https://github.com/Carable/Carable.Swagger.DocumentWithCode",
    "summary": "Allows you to customize your swagger documentation using code",
    "tags": [
      "swagger",
      "documentation",
      "discovery",
      "help",
      "webapi",
      "aspnet",
      "aspnetcore"
    ]
  },

  "buildOptions": {
      "xmlDoc": true
  },

  "dependencies": {
    "Swashbuckle.AspNetCore.Swagger": "1.0.0",
    "Swashbuckle.AspNetCore.SwaggerUI": "1.0.0",
    "Swashbuckle.AspNetCore.SwaggerGen": "1.0.0"
  },

  "frameworks": {
    "net46": {
      "frameworkAssemblies": {
        "System.Xml": ""
      }
    },
    "netstandard1.6": {
      "dependencies": {
        "System.Xml.XPath": "4.0.1" 
      }
    }
  }
}



	

	
